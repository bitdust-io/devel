{
    "default_image": "bitdust/app",
    "ports_offset": 400,
    "pause_before": 15,
    "containers": {
        "id-a": {
            "ports": "10001:22",
            "node": {
                "role": "identity-server",
                "name": "id-a"
            }
        },
        "dht-1": {
            "ports": "10011:22",
            "node": {
                "role": "dht-seed",
                "name": "dht-1",
                "attached_layers": "2,3",
                "known_dht_seeds": "genesis"
            }
        },
        "dht-2": {
            "links": [
                "dht-1"
            ],
            "ports": "10012:22",
            "node": {
                "role": "dht-seed",
                "name": "dht-2",
                "attached_layers": "2,3",
                "known_dht_seeds": "dht-1:14441"
            }
        },
        "dht-3": {
            "links": [
                "dht-1",
                "dht-2"
            ],
            "ports": "10013:22",
            "node": {
                "role": "dht-seed",
                "name": "dht-3",
                "attached_layers": "2,3",
                "known_dht_seeds": "dht-1:14441,dht-2:14441"
            }
        },
        "stun-1": {
            "links": [
                "dht-2",
                "dht-3"
            ],
            "ports": "10021:22",
            "node": {
                "role": "stun-server",
                "name": "stun-1",
                "known_dht_seeds": "dht-2:14441,dht-3:14441"
            }
        },
        "proxy-1": {
            "links": [
                "dht-2",
                "dht-3",
                "stun-1",
                "id-a"
            ],
            "ports": "10031:22",
            "node": {
                "role": "proxy-server",
                "name": "proxy-1",
                "known_id_servers": "id-a:8084:6661",
                "known_dht_seeds": "dht-2:14441,dht-3:14441"
            }
        },
        "proxy-2": {
            "links": [
                "dht-2",
                "dht-3",
                "stun-1",
                "id-a"
            ],
            "ports": "10032:22",
            "node": {
                "role": "proxy-server",
                "name": "proxy-2",
                "known_id_servers": "id-a:8084:6661",
                "known_dht_seeds": "dht-2:14441,dht-3:14441"
            }
        },
        "supplier-1": {
            "links": [
                "dht-2",
                "dht-3",
                "stun-1",
                "id-a"
            ],
            "ports": "10041:22",
            "node": {
                "role": "supplier",
                "name": "supplier-1",
                "known_id_servers": "id-a:8084:6661",
                "known_dht_seeds": "dht-2:14441,dht-3:14441",
                "preferred_routers": "http://id-a:8084/proxy-1.xml"
            }
        },
        "supplier-2": {
            "links": [
                "dht-2",
                "dht-3",
                "stun-1",
                "id-a"
            ],
            "ports": "10042:22",
            "node": {
                "role": "supplier",
                "name": "supplier-2",
                "known_id_servers": "id-a:8084:6661",
                "known_dht_seeds": "dht-2:14441,dht-3:14441",
                "preferred_routers": "http://id-a:8084/proxy-2.xml"
            }
        },
        "supplier-3": {
            "links": [
                "dht-2",
                "dht-3",
                "stun-1",
                "id-a"
            ],
            "ports": "10043:22",
            "node": {
                "role": "supplier",
                "name": "supplier-3",
                "known_id_servers": "id-a:8084:6661",
                "known_dht_seeds": "dht-2:14441,dht-3:14441",
                "preferred_routers": "http://id-a:8084/proxy-1.xml"
            }
        },
        "supplier-4": {
            "links": [
                "dht-2",
                "dht-3",
                "stun-1",
                "id-a"
            ],
            "ports": "10044:22",
            "node": {
                "role": "supplier",
                "name": "supplier-4",
                "known_id_servers": "id-a:8084:6661",
                "known_dht_seeds": "dht-2:14441,dht-3:14441",
                "preferred_routers": "http://id-a:8084/proxy-2.xml"
            }
        },
        "customer-backup": {
            "links": [
                "dht-2",
                "dht-3",
                "stun-1",
                "id-a"
            ],
            "ports": "10051:22",
            "volumes": [
                "customer_backup:/customer_backup"
            ],
            "node": {
                "role": "customer",
                "name": "customer-backup",
                "block_size": "10 KB",
                "join_network": true,
                "num_suppliers": 2,
                "min_servers": 2,
                "max_servers": 2,
                "known_id_servers": "id-a:8084:6661",
                "known_dht_seeds": "dht-2:14441,dht-3:14441",
                "preferred_routers": "http://id-a:8084/proxy-1.xml",
                "supplier_candidates": "http://id-a:8084/supplier-1.xml,http://id-a:8084/supplier-2.xml"
            }
        },
        "customer-restore": {
            "links": [
                "dht-2",
                "dht-3",
                "stun-1",
                "id-a"
            ],
            "ports": "10052:22",
            "volumes": [
                "customer_restore:/customer_restore"
            ],
            "node": {
                "role": "customer",
                "name": "customer-restore",
                "join_network": false,
                "num_suppliers": 2,
                "known_id_servers": "id-a:8084:6661",
                "known_dht_seeds": "dht-2:14441,dht-3:14441",
                "preferred_routers": "http://id-a:8084/proxy-2.xml"
            }
        }
    },
    "containers_volumes": [
        "customer_backup:",
        "customer_restore:"
    ],
    "tester_links": [
        "dht-1",
        "dht-2",
        "dht-3",
        "stun-1",
        "id-a",
        "supplier-1",
        "supplier-2",
        "supplier-3",
        "supplier-4",
        "customer-backup",
        "customer-restore"
    ],
    "tester_volumes": [
        "customer_backup:/customer_backup",
        "customer_restore:/customer_restore"
    ]
}